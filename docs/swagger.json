{
    "swagger": "2.0",
    "info": {
        "description": "Todo list API using Fiber and Postgres",
        "title": "The ultimate todo list API",
        "contact": {
            "name": "Bonvic Bundi"
        },
        "license": {
            "name": "MIT"
        },
        "version": "0.1"
    },
    "host": "localhost:4500",
    "basePath": "/",
    "paths": {
        "/api/v1/todos": {
            "post": {
                "description": "Be able to create a Todo",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "create a Todo",
                "parameters": [
                    {
                        "description": "Todo",
                        "name": "todo",
                        "in": "body",
                        "schema": {
                            "$ref": "#/definitions/todo.ReturnedTodo"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/todo.TodoResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "todo.ReturnedTodo": {
            "type": "object",
            "properties": {
                "completed": {
                    "type": "boolean"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "todo.TodoResponse": {
            "type": "object"
        }
    }
}